  "item": [
    {
      "name": "ADAMO Creation",
      "item": [
        {
          "name": "Create MAP_INITIAL",
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"grNumber\": \"GR-25-0001-1\",\n  \"evaluationDate\": \"2025-11-03T10:00:00Z\",\n  \"chemist\": \"Smith\",\n  \"assessor\": \"Johnson, Williams\",\n  \"dilution\": \"10% in DPG\",\n  \"evaluationSite\": \"US\",\n  \"odor0H\": \"Fresh, citrus notes with slight floral undertones, clean and bright initial impression\",\n  \"odor4H\": \"Mellowed citrus, emerging woody notes, balanced sweetness\",\n  \"odor24H\": \"Woody base with lingering sweet notes, minimal citrus\",\n  \"comments\": \"Molecule shows good evolution over time. Consider for CP track evaluation.\",\n  \"createdBy\": \"APIUSER\"\n}"
            },
            "url": {
              "raw": "{{baseUrl}}/adamo/initial",
              "host": ["{{baseUrl}}"],
              "path": ["adamo", "initial"]
            },
            "description": "Creates a new MAP_INITIAL record with molecule evaluation data"
          },
          "response": []
        },
